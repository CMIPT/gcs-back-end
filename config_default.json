{
    "profiles": [
        "prod"
    ],
    "deployLogLevel": "info",
    "adminName": "gcs-admin",
    "adminEmail": "gcs-admin@localhost",
    "gitUserName": "git",
    "gitHomeDirectory": "/home/git",
    "gitUserPassword": "git",
    "gitServerDomain": "localhost",
    "gitServerPort": 22,
    "localSshdPort": 22,
    "deployWithDocker": false,
    "dockerName": "gcs-back-end",
    "dockerImage": "ubuntu:20.04",
    "dockerPortMapping": [
        "8080:8080"
    ],
    "dockerWithGpu": false,
    "dockerSrcPath": "/opt/gcs-back-end-src",
    "serviceType": "sys-init-v",
    "serviceName": "gcs",
    "serviceDescription": "Git server center back-end service",
    "servicePIDFile": "/var/run/gcs.pid",
    "serviceUser": "gcs",
    "serviceUserPassword": "gcs",
    "serviceUserHomeDirectory": "/home/gcs",
    "serviceStartJavaCommand": "/usr/bin/java",
    "serviceStartJavaArgs": [
        "-jar"
    ],
    "serviceStartJarFile": "/opt/gcs/gcs.jar",
    "serviceStartWithBoot": true,
    "serviceSuffix": ".service",
    "serviceWorkingDirectory": "/opt/gcs",
    "serviceRestartPolicy": "always",
    "serviceRestartDelaySeconds": 5,
    "serviceAfter": [
        "network.target"
    ],
    "serviceWantedBy": [
        "multi-user.target"
    ],
    "serviceSystemdDirectory": "/etc/systemd/system",
    "serviceSysVInitDirectory": "/etc/init.d",
    "serviceLogFile": "/tmp/log/gcs.log",
    "postgresUserPassword": "postgres",
    "postgresqlUserName": "postgres",
    "postgresqlUserPassword": "postgres",
    "postgresqlDatabaseName": "gcs",
    "postgresqlHost": "localhost",
    "postgresqlPort": 5432,
    "druidLoginUsername": "druid",
    "druidLoginPassword": "druid",
    "deleteGitUser": true,
    "deleteServiceUser": true,
    "md5Salt": "",
    "frontEndUrl": "http://localhost:3000",
    "staticPathPattern": null,
    "staticLocations": null,
    "redisHost": "localhost",
    "redisPort": 6379
}
